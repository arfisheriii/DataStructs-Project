*Class Project                        *
*  Class to implement ArrayList       *
*  	sets up the master list           *
*  	of profiles                       *
*   Andy Fisher 4/8/2016              *
***************************************/

import java.util.*;

public class MasterList {
	
	private ArrayList masterBag; 
	private String name;
	private String status;
	
	public MasterList(String name, String status){
	
		ArrayList masterBag = new ArrayList();		
	}

    public void add(String n, String s) {
    	
        	masterBag.add(n, s);
    }

    public void logOn(String n) {
    	
    	masterBag.login(n);
    }

    public void masterAdd(String n, String s) {
    	
        masterBag.addToMaster(n);
    }

    public String getFriend(String n) {
    	
        return masterBag.sortedSearch(n);
    }

    public void setFriend(String n) {
    	masterBag.addAsFriend(n);
    }
}
