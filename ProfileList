***************************************/
import java.util.*;

public class ProfileList {
	
	private ArrayList profileBag; 
	private String name;
	private String status;
	
	public ProfileList(String name, String status){
	
		ArrayList profileBag = new ArrayList();		
	}

    public void add(String n, String s) {
    	
    	profileBag.add(n, s);
    }

    public void logOn(String n) {
    	
    	profileBag.login(n);
    }

    public void masterAdd(String n, String s) {
    	
        profileBag.addToMaster(n, s);
    }

    public String getFriend(String n) {
    	
        return profileBag.sortedSearch(n);
    }

    public void setFriend(String n) {
    	
    	profileBag.addFriend(n);
    }
}
